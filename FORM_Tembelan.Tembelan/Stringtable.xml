			<Original>Request %2</Original>
			<French>Demander %2</French>
		</Key>
		<Key ID="STR_GSRI_selectable_heliRemove">
			<Original>Remove %1</Original>
			<French>Supprimer %1</French>
		</Key>
		<Key ID="STR_GSRI_selectable_heliEquipFRIES">
			<Original>Equip FRIES</Original>
			<French>Equiper FRIES</French>
		</Key>
